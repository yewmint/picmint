{
  "name": "picmint",
  "version": "0.0.0",
  "description": "tag based picture management application",
  "main": "./dist/main.js",
  "build": {
    "appId": "me.yewmint.picmint",
    "directories": {
      "app": "dist",
      "output": "bundle"
    },
    "nodeGypRebuild": false,
    "npmRebuild": true,
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": "x64"
        },
        {
          "target": "appx",
          "arch": "x64"
        }
      ]
    }
  },
  "scripts": {
    "start": "electron .",
    "lint": "eslint src",
    "clean": "rimraf dist/*",
    "rebuild": "yarn run clean && yarn run build",
    "build": "yarn run lint && yarn run build:main && yarn run build:renderer",
    "build:main": "cross-env NODE_ENV=production webpack -p --config config/main.config.production.js",
    "dev:main": "webpack --config config/main.config.js",
    "build:renderer": "cross-env NODE_ENV=production webpack -p --config config/renderer.config.production.js",
    "dev:renderer": "webpack-dev-server --config config/renderer.config.js",
    "electron:rebuild": "electron-rebuild",
    "pack": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yewmint/picmint.git"
  },
  "keywords": [
    "picture",
    "management",
    "application",
    "tag"
  ],
  "author": "yewmint",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yewmint/picmint/issues"
  },
  "homepage": "https://github.com/yewmint/picmint#readme",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-version-inline": "^1.0.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.22.0",
    "copy-webpack-plugin": "^4.2.3",
    "copyfiles": "^1.2.0",
    "cross-env": "^3.1.4",
    "electron": "^1.7.9",
    "electron-builder": "^19.49.0",
    "electron-rebuild": "^1.6.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "make-dir-webpack-plugin": "^0.1.0",
    "node-gyp": "^3.6.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "rimraf": "^2.6.0",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0",
    "webpack-node-externals": "^1.6.0",
    "webpack-synchronizable-shell-plugin": "^0.0.6"
  },
  "dependencies": {
    "animejs": "^2.2.0",
    "babel-eslint": "^7.1.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-stage-2": "^6.24.1",
    "classname": "^0.0.0",
    "color": "^2.0.1",
    "debug": "^3.1.0",
    "eslint": "^3.14.1",
    "eslint-plugin-react": "^6.9.0",
    "immutable": "^3.8.2",
    "jss": "^9.3.3",
    "jss-nested": "^6.0.1",
    "jss-preset-default": "^4.0.1",
    "lodash": "^4.17.4",
    "moment": "^2.19.3",
    "prop-types": "^15.6.0",
    "react": "^15.4.2",
    "react-addons-pure-render-mixin": "^15.6.2",
    "react-dom": "^15.4.2",
    "react-icons": "^2.2.7",
    "react-imageloader": "^2.1.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.2.1",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "sharp": "^0.18.4"
  }
}
