{
  "name": "picmint",
  "version": "0.0.0",
  "description": "tag based picture management application",
  "main": "./dist/main.js",
  "scripts": {
    "start": "electron .",
    "lint": "eslint src",
    "clean": "rimraf dist",
    "rebuild": "yarn run clean && yarn run build",
    "build": "yarn run lint && yarn run build:main && yarn run build:renderer",
    "build:main": "cross-env NODE_ENV=production webpack -p --config config/main.config.js",
    "dev:main": "webpack --config config/main.config.js",
    "build:renderer": "cross-env NODE_ENV=production webpack -p --config config/renderer.config.js",
    "dev:renderer": "webpack-dev-server --config config/renderer.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yewmint/picmint.git"
  },
  "keywords": [
    "picture",
    "management",
    "application",
    "tag"
  ],
  "author": "yewmint",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yewmint/picmint/issues"
  },
  "homepage": "https://github.com/yewmint/picmint#readme",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-version-inline": "^1.0.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.22.0",
    "copy-webpack-plugin": "^4.2.3",
    "copyfiles": "^1.2.0",
    "cross-env": "^3.1.4",
    "electron": "^1.7.9",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "rimraf": "^2.6.0",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "babel-eslint": "^7.1.1",
    "classname": "^0.0.0",
    "eslint": "^3.14.1",
    "eslint-plugin-react": "^6.9.0",
    "jss": "^9.3.3",
    "jss-nested": "^6.0.1",
    "jss-preset-default": "^4.0.1",
    "prop-types": "^15.6.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-icons": "^2.2.7"
  }
}
